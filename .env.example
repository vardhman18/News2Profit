# Environment Configuration Template for NEWS2PROFIT
# Copy this file to .env and fill in your actual API keys

# SerpApi (Google News) Configuration
NEWS_API_KEY=your_serpapi_key_here

# Reddit API Configuration  
REDDIT_CLIENT_ID=your_reddit_client_id_here
REDDIT_CLIENT_SECRET=your_reddit_client_secret_here
REDDIT_USER_AGENT=NEWS2PROFIT/1.0

# Optional: Set to true to disable specific data sources
DISABLE_TWITTER_FETCH=true